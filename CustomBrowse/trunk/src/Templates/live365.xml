<?xml version="1.0" encoding="utf-8"?>
<custombrowse>
	<requireplugins>Live365</requireplugins>
	<template>
		<downloadidentifier>92</downloadidentifier>
		<name>Live365 Radio</name>
		<description>Menu that puts the standard Live365 Radio menu below Custom Browse menu</description>
		<parameter type="text" id="menuname" name="Menu name" value="Live365 Radio"/>
		<parameter type="text" id="menugroup" name="Menu group" value=""/>
		<parameter type="text" id="menuorder" name="Menu sort order (1-100)" value="50"/>
		<parameter type="text" id="includedclients" name="Only include on these players (empty = all)" value=""/>
		<parameter type="text" id="excludedclients" name="Exclude from these players" value=""/>
	</template>
</custombrowse>

