<?xml version="1.0" encoding="utf-8"?>
<custombrowse>
	<minslimserverversion>6.5</minslimserverversion>
	<mix>
		<mixname>Browse by selected</mixname>
		<mixcategory>year</mixcategory>
		<mixtype>function</mixtype>
		<mixsupport>web,player,jive</mixsupport>
		<mixchecktype>function</mixchecktype>
		<mixcheckdata>Plugins::CustomBrowse::Plugin::browseByMixable</mixcheckdata>
		<mixdata>Plugins::CustomBrowse::Plugin::browseByMix</mixdata>
		<mixurl>plugins/CustomBrowse/custombrowse_contextlist.html?contextname={itemname}&amp;contexttype={itemtype}&amp;contextid={itemid}</mixurl>
		<mixjive>custombrowse browsejivecontext contextid:{itemid} contexttype:{itemtype}</mixjive>
		<miximage>html/images/browsebyselected.gif</miximage>
	</mix>
</custombrowse>
